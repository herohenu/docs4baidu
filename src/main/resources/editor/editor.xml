    <bean id="zohoWriterEditor" class="com.baihui.docs4baidu.editor.entity.Editor">
        <property name="name" value="zohoWriterEditor"/>
        <property name="serverUrl" value="${editor.zoho.writer.serverUrl}"/>
        <property name="apikey" value="${editor.zoho.apikey}"/>
        <property name="format" value="${editor.writer.format}"/>
        <property name="persistence" value="false"/>
        <property name="id" value="123456789"/>
        <property name="saveUrl" value="${editor.saveUrl}"/>
        <property name="com" value="zoho"/>
    </bean>

